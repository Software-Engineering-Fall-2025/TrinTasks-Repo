{
  "name": "iCal Parser",
  "description": "Parse and display information from iCal links",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "iCal Parser"
  },
  "background": {
    "service_worker": "background.js"
  }
}
